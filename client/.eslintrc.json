{
  "extends": ["next/core-web-vitals", "airbnb", "prettier", "plugin:prettier/recomended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "max-len": ["error", 140],
    "quotes": [2, "single", { "avoidEscape": true }]
  }
}
